{
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:cypress/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["cypress", "prettier", "import"],
  "rules": {
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": 0,
  }
}
